      "adaptiveIcon": {
        "foregroundImage": "./assets/logo_chatroom.png",
        "backgroundColor": "#eed611"
      },
      "package": "com.paulbabu05.chatapp"
    },
    "web": {
      "favicon": "./assets/logo_chatroom.png"
    },
    "description": "A ChatApp Developed By Paul Babu"
  }
