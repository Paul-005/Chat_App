{
  "expo": {
    "name": "Chat-App",
    "slug": "chat-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo_chatroom.png",
    "splash": {
      "image": "./assets/logo_chatroom.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
